-- int i = 5;
Const 5 RegA
, Push RegA -- Save all values onto stack immediately for now

-- If statement
-- From expr (left side of > expr)
, Load (Deref SP) RegA -- Read it back from the stack because we need it for the if statement
-- From expr (right side of > expr)
, Const 3 RegB -- Create register with value 3 to use in if statement
-- From > expr
, Compute Gt RegA RegB RegA
-- From if statement
--, Branch RegA (Rel 2) ?
, Branch RegA (Rel 1)
, Jump (Rel 3)

-- Inside of if statement block
, Const 10 RegA
, Store RegA (Deref SP)
-- Else part of if
, Const (ord 'N') RegE
, Write RegE stdio
-- Rest of code
, EndProg